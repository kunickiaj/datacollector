    "@bower_components/angular-scenario": "angular/bower-angular-scenario#1.6.10",
    "@bower_components/angular-translate": "PascalPrecht/bower-angular-translate#2.15.2",
    "@bower_components/angular-translate-loader-static-files": "PascalPrecht/bower-angular-translate-loader-static-files#2.15.2",
    "@bower_components/angular-translate-storage-cookie": "PascalPrecht/bower-angular-translate-storage-cookie#2.15.2",
    "@bower_components/angular-ui-select": "angular-ui/ui-select#0.13.2",
    "@bower_components/angular-xeditable": "vitalets/angular-xeditable#~0.1.9",
    "@bower_components/bootstrap": "twbs/bootstrap#~3.3.1",
    "@bower_components/codemirror": "components/codemirror#5.38.0",
