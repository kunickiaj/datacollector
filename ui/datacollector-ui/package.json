    "@bower_components/fontawesome": "FortAwesome/Font-Awesome#~4.2.0",
    "@bower_components/jquery": "jquery/jquery-dist#2.1.1",
    "@bower_components/moment": "moment/moment#2.9.0",
    "@bower_components/ngstorage": "gsklee/ngStorage#0.3.7",
